{
  "name": "timtam-infra",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -w -p tsconfig.json",
    "cdk": "cdk",
    "synth": "cdk synth --profile admin",
    "deploy": "cdk deploy --profile admin --require-approval never",
    "destroy": "cdk destroy --profile admin",
    "synth-deploy": "pnpm run synth && pnpm run deploy",
    "sso:admin": "aws sso login --profile admin"
  },
  "devDependencies": {
    "aws-cdk": "2.165.0",
    "aws-cdk-lib": "2.165.0",
    "constructs": "10.3.0",
    "@types/node": "20.11.25",
    "esbuild": "0.25.0",
    "ts-node": "10.9.2",
    "typescript": "5.6.3"
  },
  "packageManager": "pnpm@10.25.0"
}
