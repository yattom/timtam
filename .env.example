# Admin API password for infrastructure management
# Used by /admin/close and /admin/open endpoints
ADMIN_PASSWORD=your-secret-password-here

# Recall.ai API KEY
RECALL_API_KEY=abcdefghijklmnopqrstuvwxyz01234567899999

# Webhook URL for Recall.ai.  This will be changed when CDK stack is recreated.
# Don't forget to add /recall/webhook
RECALL_WEBHOOK_URL=https://example.com/recall/webhook

# Recall.ai transcription provider configuration
# Options: recallai_streaming, deepgram_streaming
RECALL_TRANSCRIPTION_PROVIDER=deepgram_streaming

# Language setting for transcription
# Options: auto (automatic language detection), ja (Japanese), en (English), etc.
RECALL_TRANSCRIPTION_LANGUAGE=auto
